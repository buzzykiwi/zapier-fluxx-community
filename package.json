{
  "name": "zapier-fluxx-community",
  "version": "1.0.1",
  "description": "Third-party Zapier application for Fluxx",
  "main": "index.js",
  "scripts": {
    "test2": "mocha --recursive -t 10000",
    "test": "jest --testTimeout 10000",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "2engines": {
    "node": ">=v14",
    "npm": ">=5.6.0"
  },
  "dependencies": {
    "jest": "^29.3.1",
    "minimatch": "^7.4.2",
    "request": "^2.88.2",
    "yarn": "^1.22.19",
    "zapier-platform-core": "13.0.0"
  },
  "devDependencies": {
    "mocha": "^10.2.0",
    "should": "^13.2.0"
  },
  "private": true,
  "zapier": {
    "convertedByCLIVersion": "12.2.0"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/test/**/*.[jt]s?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "/triggers/",
      "/DISABLED/"
    ]
  }
}
